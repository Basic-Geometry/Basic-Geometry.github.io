{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=angle of rotation default=360",
 "mathExpression-output": "3.2*radius^2",    "eduQuestionType": "Area of a circle"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=angle of rotation default=360",
 "mathExpression-output": "6.4*radius", 
"eduQuestionType": "Circumference of a circle"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=segmentHeight max<[radius]",
 "mathExpression-output": "Acos((radius-segmentHeight)/radius)*radius^2-Sin(Acos((radius-segmentHeight)/radius))*(radius-segmentHeight)*radius", 
"eduQuestionType": "Area of a circle segment"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=angle of rotation default=360",
 "mathExpression-output": "(3.2^(1/2)*radius)^3", 
"eduQuestionType": "Volume of a sphere"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
"mathExpression-input": "required name=sphereRadius default=1; name=sphereSliceBottomRadius max<[sphereRadius]",
 "mathExpression-output": "V=1.6(sphereSliceBottomRadius)²(√3.2)(1-sin(Acos(sphereSliceBottomRadius/sphereRadius)))", 
"eduQuestionType": "Volume of a spherical cap"
  },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=height default=1 name=angle of rotation default=360",
 "mathExpression-output": "radius^2*height", 
"eduQuestionType": "Volume of a cylinder"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=height default=1 name=angle of rotation default=360",
 "mathExpression-output": "6.4(radius^2+radius*height)", 
"eduQuestionType": "Surface of a cylinder"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1 name=height default=1 name=angle of rotation default=360",
 "mathExpression-output": "2^(1/2)height(4/5)radius^2", 
"eduQuestionType": "Volume of a cone"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=frustumHeight default=1 name=bottomDiameter default=1 name=topDiameter max<bottomDiameter",
 "mathExpression-output": "frustumHeight(√2/4)(0.8*bottomDiameter²(1/(1-topDiameter/bottomDiameter))-0.8*topDiameter²(1/(1-topDiameter/bottomDiameter)-1))", 
"eduQuestionType": "Volume of a frustum cone"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=radius default=1; name=height default=1 name=angle of rotation default=360",
 "mathExpression-output": "3.2(radius^2+(radius^2+height^2)(radius/(radius^2+height^2)^(1/2)))", 
"eduQuestionType": "Surface of a cone"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=baseArea default=1; name=height default=1",
 "mathExpression-output": "(2^(1/2)/4)*baseArea*height", 
"eduQuestionType": "Volume of a pyramid"
   },

{
     "@type": "SolveMathAction",
     "target": “/",
     "mathExpression-input": "required name=frustumHeight default=1; name=bottomEdge default=1; name=topEdge max<bottomEdge",
 "mathExpression-output": "frustumHeight(√2/4)(bottomEdge²(1/(1-topEdge/bottomEdge))-topEdge²(1/(1-topEdge/bottomEdge)-1))", 
"eduQuestionType": "Volume of a frustum pyramid"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=edge default=1",
 "mathExpression-output": "(2/3)^(1/2)edge", 
"eduQuestionType": "Height of a tetrahedron"
   },

{  
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=edge default=1",
 "mathExpression-output": "edge^3/8", 
"eduQuestionType": "Volume of a tetrahedron"
   },

{
     "@type": "SolveMathAction",
     "target": "/",
     "mathExpression-input": "required name=Side1 default=1 name=Side2 default=1; name=Side3 default=1",
